   "source": [
    "print(\"원피스 몇 개를 구매하시겠습니까?\")\n",
    "op = int(input())\n",
    "print(\"스웨터 몇 개를 구매하시겠습니까?\")\n",
    "sw = int(input())\n",
    "total = op*50000 + sw*30000\n",
    "if total < 100000:\n",
    "    total = total*0.95\n",
    "elif 100000 <= total < 200000:\n",
    "    total = total*0.9\n",
    "else:\n",
    "    total = total*0.8\n",
    "print(\"총액:{0}\".format(total))\n"
